type error = {
    code: int;
    message: string;
}

type failure = {
    id: int;
    error: error;
}

type response = {
    id: int;
    result: abstract;
}

type init_params = {
    ~debug <python default="False">: bool;
    root: string;
}

type init_request = {
    ~method <python default="'petanque/init'">: string;
    id: int;
    params: init_params;
}

type init_response = {
    id: int;
    result: int; 
}

type start_params = {
    env: int;
    uri: string;
    thm: string;
}

type start_request = {
    ~method <python default="'petanque/start'">: string;
    id: int;
    params: start_params;
}

type run_params = {
    st: int;
    tac: string;
}

type run_request = {
    ~method <python default="'petanque/run'">: string;
    id: int;
    params: run_params;
}

type run_response = [ Current_state of int | Proof_finished of int ]

type goals_params = {
    st: int;
}

type goals_request = {
    ~method <python default="'petanque/goals'">: string;
    id: int;
    params: goals_params;
}

type goals_response = {
    goals : abstract list;
}

type premises_params = {
    st: int;
}

type premises_request = {
    ~method <python default="'petanque/premises'">: string;
    id: int;
    params: premises_params;
}

type premises_response = abstract
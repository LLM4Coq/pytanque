type error = {
    code: int;
    message: string;
}

type failure = {
    id: int;
    error: error;
}

type response = {
    ~jsonrpc <python default="'2.0'">: string;
    id: int;
    result: abstract;
}

type request = {
    ~jsonrpc <python default="'2.0'">: string;
    id: int;
    method_ <json name="method">: string;
    params: abstract
}

type init_params = {
    ~debug <python default="False">: bool;
    root: string;
}

type start_params = {
    env: int;
    uri: string;
    thm: string;
}

type run_params = {
    st: int;
    tac: string;
}

type run_response = [ Current_state of int | Proof_finished of int ]

type goals_params = {
    st: int;
}

type goals_response = {
    goals : abstract list;
}

type premises_params = {
    st: int;
}

type premises_response = abstract